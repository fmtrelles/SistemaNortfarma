package sistemanortfarma;

/**
 *
 * @author Miguel Gomez S. gomez
 */
public class VerificaSistema {

    private static String sSistemaOperativo;

    public VerificaSistema(){
        sSistemaOperativo = System.getProperty("os.name");
        this.setsSistemaOperativo(sSistemaOperativo);
    }

    public static String getsSistemaOperativo() {
        return sSistemaOperativo;
    }

    public static void setsSistemaOperativo(String sSistemaOperativo) {
        VerificaSistema.sSistemaOperativo = sSistemaOperativo;
    }

}
