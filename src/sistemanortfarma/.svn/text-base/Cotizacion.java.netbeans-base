            String recTipDoc = String.valueOf(lista[1]); // recupera el id de tipo de doc

            /* if (recTipDoc.equals("1") ){
            canti = canti +1;
            canti1 = Integer.parseInt(lista[0]);
            canti1 = canti1 + 7;
            canti2 = Integer.parseInt(lista[0]);
            canti2 = canti2 + 13;

            cantidad1 = String.valueOf(canti);
            cantidad2 = String.valueOf(canti1);
            cantidad3 = String.valueOf(canti2);
            //valueCompara = String.valueOf(jTextField13.getText().charAt(0));
            }else if (recTipDoc.equals("2")){
            canti = canti +1;
            canti1 = Integer.parseInt(lista[0]);
            canti1 = canti1 + 13;
            canti2 = Integer.parseInt(lista[0]);
            canti2 = canti2 + 25;

