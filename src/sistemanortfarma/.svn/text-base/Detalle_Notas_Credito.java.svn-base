    /** Creates new form Detalle_Notas_Credito */
    public Detalle_Notas_Credito(String doc, String idboti, NotasCredito notacred) {
        initComponents();
        this.setLocationRelativeTo(null);
        model = (DefaultTableModel) this.jTable1.getModel();
        colModel1 = jTable1.getColumnModel();
        num_doc = doc;
        idbotica = idboti;
        entidadnota = notacred;
        MuestraCabecera();
        MuestraDetalle();
    }

    public void run() {
        String sistema = "Windows";

        try {

            parameters.put("IDBOTICA", idbotica);
            parameters.put("INTERNO", entidadnota.getNumero_Documento());
            parameters.put("REPORT_CONNECTION", con);
            report_file = this.getClass().getResource("/Reportes/Nota_Credito.jasper");

            if (obj.getsSistemaOperativo().indexOf(sistema) != -1) {
                parameters.put("SUBREPORT_DIR", "Reportes/");
            } else {
                parameters.put("SUBREPORT_DIR", "//Reportes//");
            }

            //   report_file = System.getProperty("user.dir")+"\\src\\Reportes\\VentasxVendedor.jasper";
            masterReport = (JasperReport) JRLoader.loadObject(report_file);
            jasperPrint = JasperFillManager.fillReport(masterReport, parameters, con);
            JasperViewer view = new JasperViewer(jasperPrint, false);
            view.setTitle("NOTA DE CREDITO");
            view.setVisible(true);
            view.setSize(850, 650);

        } catch (JRException ex) {
            System.out.println(ex.getMessage());
        }

        jButton2.setEnabled(true);

    }

    private void MuestraCabecera() {
        this.jTextField9.setText("APROBADO");
        this.jTextField1.setText(entidadnota.getSerie());
        this.jTextField2.setText(entidadnota.getNumero());
        this.jTextField3.setText(entidadnota.getSerie());
        this.jTextField4.setText(entidadnota.getNumero());
        this.jTextField5.setText(entidadnota.getFecha_Documento().toString());
        this.jTextField6.setText(entidadnota.getFecha_Documento().toString());
        this.jTextField7.setText(entidadnota.getHora().toString());
        this.jTextField10.setText(entidadnota.getResponsable());
        this.jTextArea1.setText(entidadnota.getObservaciones());
        this.jTextField8.setText(entidadnota.getId_Venta_Nota_Credito());

        this.jTextField11.setText(entidadnota.getCliente());
        this.jTextField12.setText(entidadnota.getDireccion());
        this.jTextField14.setText(entidadnota.getRuc());
        this.jTextField13.setText(entidadnota.getDni());

    }

    private void MuestraDetalle() {
        milista = logcreditos.Lista_Detalle_NotasCredito(idbotica, num_doc);

        for (int i = 0; i < milista.size(); i++) {
            datosDetalle[0] = i + 1;
            datosDetalle[1] = milista.get(i).getProducto();
            datosDetalle[2] = milista.get(i).getUnd();
            datosDetalle[3] = milista.get(i).getFracc();
            datosDetalle[4] = milista.get(i).getPvp();
            model.addRow(datosDetalle);
        }

    }

