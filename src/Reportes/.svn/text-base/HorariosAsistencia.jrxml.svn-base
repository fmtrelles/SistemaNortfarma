	<queryString>
		<![CDATA[SELECT DISTINCT CONCAT(p.`Apellido`," ",p.`Nombre`) AS Trabajador, p.`DNI`, r.`Descripcion`
FROM `Horario_Marcacion` hm, `Horario_Trabajo` ht, `Horarios_Detalle` hd, personal p, `roles` r
WHERE hm.`Id_Horario_Trabajo` = ht.`Id_Horario_Trabajo`
AND ht.`Id_Horario_Detalle` = hd.`Id_Horario_Detalle`
AND p.`Id_Personal` = hd.`Id_Personal`
AND r.`Id_Rol` = hd.`Id_Rol`
AND hd.`Id_Personal` = $P{vPersonal} ORDER BY hm.`Fecha`]]>
	</queryString>
