	<queryString>
		<![CDATA[select 	uniCar, fraCar,  pc.Descripcion as "prodCar", montCar,
	uniDes, fraDes , pd.Descripcion as "prodDes", montDes,
	diferencia, totalDiferencia

from CruceDetalle cd

inner join CruceInv ci on cd.idCruce = ci.idCruce
inner join productos pc on pc.Id_Producto = cd.codCar
inner join productos pd on pd.Id_Producto = cd.codDes

where  diferencia > 0 and ci.nombreCruce = $P{idCruce}]]>
	</queryString>
