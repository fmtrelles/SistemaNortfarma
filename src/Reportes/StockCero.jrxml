	<queryString>
		<![CDATA[SELECT Almacen_Stock_Empaque, Almacen_Stock_Fraccion, Mostrador_Stock_Empaque, Mostrador_Stock_Fraccion FROM movimientos
INNER JOIN movimientos_detalles
on movimientos.Id_Botica = movimientos_detalles.Id_Botica
AND movimientos.Id_TipoAlmacen = movimientos_detalles.Id_TipoAlmacen
AND movimientos.Id_TipoMovimiento = movimientos_detalles.Id_TipoMovimiento
AND movimientos.Id_Proveedor = movimientos_detalles.Id_Proveedor
AND movimientos.Numero_Documento = movimientos_detalles.Numero_Documento
INNER JOIN Tipo_Movimiento
on Tipo_Movimiento.Id_TipoMovimiento = movimientos.Id_TipoMovimiento
INNER JOIN boticas on boticas.Id_Botica = movimientos.Id_Botica
inner join personal on personal.Id_Personal= movimientos.Id_Personal
inner join productos on productos.Id_Producto = movimientos_detalles.Id_Producto WHERE movimientos.Fecha_Registro < $P{xInicio} and productos.Id_Producto like $P{producto}  and movimientos.Id_Botica like $P{Id_Botica} order by movimientos.Fecha_Registro DESC LIMIT 0,1]]>
	</queryString>
