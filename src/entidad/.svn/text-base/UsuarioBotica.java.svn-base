package entidad;

/**
 *
 * @author Miguel Gomez S.
 */
public class UsuarioBotica {

    private int Id_Personal;
    private int idrol, idpagina;
    private String Nombre;
    private String Apellido;
    private String DNI;
    private String Id_Botica;
    private String id_personal_botica;
    private String descrip_roles;
    private int ContraseñaCambiada;

    public UsuarioBotica() {
    }

    public UsuarioBotica(int Id_Personal, int idrol, int idpagina, String Nombre, String Apellido, String DNI, String desroles, int cambio) {
        this.Id_Personal = Id_Personal;
        this.idrol = idrol;
        this.idpagina = idpagina;
        this.Nombre = Nombre;
        this.Apellido = Apellido;
        this.DNI = DNI;
        this.descrip_roles = desroles;
        this.ContraseñaCambiada = cambio;
    }

    public int getContraseñaCambiada() {
        return ContraseñaCambiada;
    }

    public void setContraseñaCambiada(int ContraseñaCambiada) {
        this.ContraseñaCambiada = ContraseñaCambiada;
    }

    public String getDescrip_roles() {
        return descrip_roles;
    }

    public void setDescrip_roles(String descrip_roles) {
        this.descrip_roles = descrip_roles;
    }

    public String getApellido() {
        return Apellido;
    }

    public void setApellido(String Apellido) {
        this.Apellido = Apellido;
    }

    public String getNombre() {
        return Nombre;
    }

    public void setNombre(String Nombre) {
        this.Nombre = Nombre;
    }

    public int getIdpagina() {
        return idpagina;
    }

    public void setIdpagina(int idpagina) {
        this.idpagina = idpagina;
    }

    public int getIdrol() {
        return idrol;
    }

    public void setIdrol(int idrol) {
        this.idrol = idrol;
    }

    public String getId_personal_botica() {
        return id_personal_botica;
    }

    public void setId_personal_botica(String id_personal_botica) {
        this.id_personal_botica = id_personal_botica;
    }

    public String getDNI() {
        return DNI;
    }

    public void setDNI(String DNI) {
        this.DNI = DNI;
    }

    public String getId_Botica() {
        return Id_Botica;
    }

    public void setId_Botica(String Id_Botica) {
        this.Id_Botica = Id_Botica;
    }

    public int getId_Personal() {
        return Id_Personal;
    }

    public void setId_Personal(int Id_Personal) {
        this.Id_Personal = Id_Personal;
    }
}
