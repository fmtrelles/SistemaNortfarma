package entidad;

import java.sql.Date;

/**
 *
 * @author Miguel Gomez S.
 */
public class Abono_Efectivo {

    private Boticas botica;
    private Cajas caja;
    private Date FECHA;
    private Personal persona;
    private Billetes_Monedas moneda;
    private int Cantidad;
    private double Total;

    public int getCantidad() {
        return Cantidad;
    }

    public void setCantidad(int Cantidad) {
        this.Cantidad = Cantidad;
    }

    public Date getFECHA() {
        return FECHA;
    }

    public void setFECHA(Date FECHA) {
        this.FECHA = FECHA;
    }

    public double getTotal() {
        return Total;
    }

    public void setTotal(double Total) {
        this.Total = Total;
    }

    public Boticas getBotica() {
        return botica;
    }

    public void setBotica(Boticas botica) {
        this.botica = botica;
    }

    public Cajas getCaja() {
        return caja;
    }

    public void setCaja(Cajas caja) {
        this.caja = caja;
    }

    public Billetes_Monedas getMoneda() {
        return moneda;
    }

    public void setMoneda(Billetes_Monedas moneda) {
        this.moneda = moneda;
    }

    public Personal getPersona() {
        return persona;
    }

    public void setPersona(Personal persona) {
        this.persona = persona;
    }
}
