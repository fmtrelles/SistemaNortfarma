    public Venta(String id_Venta, Date Fecha_Venta, String Nombre_RazonSocial, String tipoventa, String tipopago, double SubTotal, double Total, String Serie, String Numero, String Direcc, String ruc, String dni, String nom, Double igv, String nomcajero, double notomar, int idcliente) {
        this.Fecha_Venta = Fecha_Venta;
        this.SubTotal = SubTotal;
        this.Serie = Serie;
        this.Numero = Numero;
        this.Total = Total;
        this.id_Venta = id_Venta;
        this.Nombre_RazonSocial = Nombre_RazonSocial;
        this.tipopago = tipopago;
        this.tipventa = tipoventa;
        this.Direccion = Direcc;
        this.RUC = ruc;
        this.Nombre = nom;
        this.IGV = igv;
        this.DNI = dni;
        this.cajero = nomcajero;
        this.notomar = notomar;
        this.Id_Cliente = idcliente;
    }

    public Venta(String id_Venta, Date Fecha_Venta, String Nombre_RazonSocial, String tipoventa, String tipopago, double SubTotal, double Total, String Serie, String Numero, String Direcc, String ruc, String dni, String nom, Double igv, String nomcajero, int idcliente) {
