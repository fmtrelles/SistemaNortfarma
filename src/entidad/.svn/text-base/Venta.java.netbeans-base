    @Override
    public int compare(Venta o1, Venta o2) {
        return Integer.parseInt(o1.orden) - Integer.parseInt(o2.orden);
    }

    @Override
    public int compareTo(Venta o) {
        return (this.orden).compareTo(o.orden);
    }
}
