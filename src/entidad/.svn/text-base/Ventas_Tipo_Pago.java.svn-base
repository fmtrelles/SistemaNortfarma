package entidad;

import java.util.Date;

/**
 *
 * @author Miguel Gomez S. gomez
 */
public class Ventas_Tipo_Pago {

    private String Id_Botica;
    private String Id_Venta;
    private int Id_TipoPago;
    private double Monto;
    private Date Fecha_Venta;
    private String NumDocumento;
    private String operador;

    public Ventas_Tipo_Pago(String Id_Botica, int Id_TipoPago, double Monto, Date Fecha_Venta, String NumDocumento, String operador) {
        this.Id_Botica = Id_Botica;
        this.Id_TipoPago = Id_TipoPago;
        this.Monto = Monto;
        this.Fecha_Venta = Fecha_Venta;
        this.NumDocumento = NumDocumento;
        this.operador = operador;
    }

    public Ventas_Tipo_Pago(String descrip, double monto) {
        this.NumDocumento = descrip;
        this.Monto = monto;
    }

    public String getOperador() {
        return operador;
    }

    public void setOperador(String operador) {
        this.operador = operador;
    }

    public Ventas_Tipo_Pago() {
    }

    public Date getFecha_Venta() {
        return Fecha_Venta;
    }

    public void setFecha_Venta(Date Fecha_Venta) {
        this.Fecha_Venta = Fecha_Venta;
    }

    public String getId_Botica() {
        return Id_Botica;
    }

    public void setId_Botica(String Id_Botica) {
        this.Id_Botica = Id_Botica;
    }

    public int getId_TipoPago() {
        return Id_TipoPago;
    }

    public void setId_TipoPago(int Id_TipoPago) {
        this.Id_TipoPago = Id_TipoPago;
    }

    public String getId_Venta() {
        return Id_Venta;
    }

    public void setId_Venta(String Id_Venta) {
        this.Id_Venta = Id_Venta;
    }

    public double getMonto() {
        return Monto;
    }

    public void setMonto(double Monto) {
        this.Monto = Monto;
    }

    public String getNumDocumento() {
        return NumDocumento;
    }

    public void setNumDocumento(String NumDocumento) {
        this.NumDocumento = NumDocumento;
    }
}
