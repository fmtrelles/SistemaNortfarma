        this.Numero_Documento = NumeroDocumento;
        this.Fecha_Documento = Fecha_Documento;
        this.Total = Total;
        this.Serie = Serie;
        this.Numero = Numero;
        this.Hora = Hora;
        this.Observaciones = OBSERVACIONES;
        this.responsable = responsable;
        this.Id_Venta_Nota_Credito = interno;
        this.cliente = cliente;
        this.direccion = direccion;
        this.ruc = ruc;
        this.dni = dni;

    }

    public NotasCredito(String Numero_Documento, double Total, String Serie, String Numero, String cliente, String idventa) {
        this.Numero_Documento = Numero_Documento;
        this.Total = Total;
        this.Serie = Serie;
        this.Numero = Numero;
        this.cliente = cliente;
        this.Id_Venta_Nota_Credito = idventa;
