package entidad;

/**
 *
 * @author Miguel Gomez S.
 */
public class Contratos {

    public String Id_Modelo_Contrato;
    public String Descripcion;

    public Contratos(String Id_Modelo_Contrato, String Descripcion) {
        this.Id_Modelo_Contrato = Id_Modelo_Contrato;
        this.Descripcion = Descripcion;
    }

    public String getDescripcion() {
        return Descripcion;
    }

    public void setDescripcion(String Descripcion) {
        this.Descripcion = Descripcion;
    }

    public String getId_Modelo_Contrato() {
        return Id_Modelo_Contrato;
    }

    public void setId_Modelo_Contrato(String Id_Modelo_Contrato) {
        this.Id_Modelo_Contrato = Id_Modelo_Contrato;
    }
}
