public class Equipos {

    String Descripcion;
    Integer Categoria;
    String Codigo_Nortfarma;
    String Codigo_Nuevo;
    String Codigo_Barras;
    Integer Id_Responsable;
    String Locacion;
    Integer Tipo_Equipo;
    String Observaciones;
    String DescripcionCategoria;
    String Responsable;

    public Equipos(String Descripcion, String DescripcionCategoria, String Codigo_Nortfarma, String Codigo_Barras, String Responsable, String Locacion, String Observaciones) {
        this.Descripcion = Descripcion;
        this.DescripcionCategoria = DescripcionCategoria;
        this.Codigo_Nortfarma = Codigo_Nortfarma;
        this.Codigo_Barras = Codigo_Barras;
        this.Responsable = Responsable;
        this.Locacion = Locacion;
        this.Observaciones = Observaciones;
    }

    public Equipos(String Codigo_Nuevo) {
        this.Codigo_Nuevo = Codigo_Nuevo;
    }

    public String getResponsable() {
        return Responsable;
    }

    public void setResponsable(String Responsable) {
        this.Responsable = Responsable;
    }

    public String getDescripcionCategoria() {
        return DescripcionCategoria;
    }

    public void setDescripcionCategoria(String DescripcionCategoria) {
        this.DescripcionCategoria = DescripcionCategoria;
    }

    public Integer getCategoria() {
        return Categoria;
    }

    public void setCategoria(Integer Categoria) {
        this.Categoria = Categoria;
    }

    public String getCodigo_Barras() {
        return Codigo_Barras;
    }

    public void setCodigo_Barras(String Codigo_Barras) {
        this.Codigo_Barras = Codigo_Barras;
    }

    public String getCodigo_Nortfarma() {
