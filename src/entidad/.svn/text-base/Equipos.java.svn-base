        this.Descripcion = Descripcion;
        this.Locacion = Locacion;
        this.Observaciones = Observaciones;
        this.DescripcionCategoria = DescripcionCategoria;
    }

    public Equipos(String Descripcion, String Codigo_Nortfarma, String Codigo_Nuevo, String Codigo_Barras, String Locacion, String Observaciones) {
        this.Descripcion = Descripcion;
        this.Codigo_Nortfarma = Codigo_Nortfarma;
        this.Codigo_Nuevo = Codigo_Nuevo;
        this.Codigo_Barras = Codigo_Barras;
        this.Locacion = Locacion;
        this.Observaciones = Observaciones;
    }
