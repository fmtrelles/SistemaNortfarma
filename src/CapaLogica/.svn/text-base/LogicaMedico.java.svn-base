package CapaLogica;

import entidad.Medico;
import java.util.List;
import CapaDatos.MedicoData;

/**
 *
 * @author Miguel Gomez S.
 */
public class LogicaMedico {

    MedicoData objmedico;

    public LogicaMedico() {
        objmedico = new MedicoData();
    }

    public List<Medico> listaMedicos() {
        return objmedico.listaMedicos();
    }

    public List<Medico> BuscaMedicos(String filtro, int opdebusqueda) {
        return objmedico.BuscaMedicos(filtro, opdebusqueda);
    }
}
